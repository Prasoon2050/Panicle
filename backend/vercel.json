   {
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",      
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://panicle.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST"
      }

    }
  ]
}
